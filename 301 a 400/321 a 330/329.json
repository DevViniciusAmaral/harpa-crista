{
  "number": 329,
  "title": "Jesus é Minha Paz",
  "data": [
    {
      "type": "verse",
      "lines": [
        "Jesus nos braços me tomou,",
        "Jesus me deu a paz;",
        "Agora canto a quem me amou,",
        "Jesus é minha paz!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Faminto e sedento andei,",
        "Sem ter descanso, aqui;",
        "Mas água viva eu aceitei;",
        "Achei descanso aqui!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Onipotente pra me amar,",
        "É Cristo, o Rei dos céus;",
        "Alegremente vou cantar;",
        "Achei descanso em Deus!"
      ]
    }
  ],
  "author": "P.L.M Paulo Leivas Macalão",
  "observation": "Pode ser que tenha outro co-autor"
}
