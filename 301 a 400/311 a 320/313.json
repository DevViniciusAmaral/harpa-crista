{
  "number": 313,
  "title": "Louvor a Trindade",
  "data": [
    {
      "type": "verse",
      "lines": [
        "Ao Pai Eterno dai louvor,",
        "Ao Filho Seu também,",
        "E ao celestial Consolador,",
        "Louvai p’ra sempre. amém!"
      ]
    }
  ],
  "author": "P.L.M Paulo Leivas Macalão",
  "observation": "Pode ser que tenha outro co-autor"
}
