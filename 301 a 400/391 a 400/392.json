{
  "number": 392,
  "title": "Peregrinos Somos",
  "data": [
    {
      "type": "verse",
      "lines": [
        "Peregrinos somos aqui",
        "Té o labor findar;",
        "Deste mundo queremos ir",
        "Ao celeste lar."
      ]
    },
    {
      "type": "chorus",
      "lines": [
        "Oh! Pátria mui feliz",
        "Em ti irei morar;",
        "Eu irei morar",
        "Eu irei morar",
        "Jerusalém, oh meu país",
        "E meu querido lar!",
        "Jerusalém, oh meu país",
        "E meu querido lar!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Minha grande consolação",
        "É sempre em ti morar,",
        "Oh, querida, bela Sião,",
        "Meu eterno lar!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Como o povo de Abraão,",
        "Sempre a viajar,",
        "Para a terra da promissão,",
        "Santo e doce lar."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Ó Senhor, queremos vencer,",
        "E de Ti alcançar",
        "Gozo, paz, eterno prazer,",
        "No bendito lar!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "É o meu desejo, Jesus,",
        "Sempre peregrinar;",
        "Dirigido por lua luz",
        "Té que chegue ao lar."
      ]
    }
  ],
  "author": "M.A.N Manuel Augusto das Neves",
  "observation": "Pode ser que tenha outro co-autor"
}
