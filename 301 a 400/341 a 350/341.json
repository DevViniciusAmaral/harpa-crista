{
  "number": 341,
  "title": "A Aspiração da Alma",
  "data": [
    {
      "type": "verse",
      "lines": [
        "Minh’alma aspira habitar",
        "Nos átrios do Senhor,",
        "E para sempre ouvir soar",
        "A voz do meu Redentor."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Descanso não se encontrará",
        "No lar do pecador;",
        "Só em Jesus se achará",
        "Justiça, paz e amor."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Ó vinde, crentes, vamos já",
        "A casa do Senhor,",
        "Pois Ele nos ensinará",
        "Servi-Lo com mui fervor!"
      ]
    }
  ],
  "author": "P.L.M Paulo Leivas Macalão",
  "observation": "Pode ser que tenha outro co-autor"
}
