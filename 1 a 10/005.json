{
  "number": 5,
  "title": "Ó Desce Fogo Santo",
  "data": [
    {
      "type": "verse",
      "lines": [
        "Espírito, alma e corpo,",
        "Oferto a Ti, Senhor,",
        "Como hóstia verdadeira,",
        "Em oblação de amor."
      ]
    },
    {
      "type": "chorus",
      "lines": [
        "Eu tudo a Deus consagro",
        "Em Cristo, o vivo altar;",
        "Ó desce, fogo santo,",
        "Do céu vem tu selar!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Sou teu, ó Jesus Cristo!",
        "Teu sangue me comprou;",
        "Eu quero a Tua graça,",
        "Pois de Ti sempre sou."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Espírito divino,",
        "Do Pai a promissão;",
        "Sedenta a alma pede,",
        "A Ti, a santa unção."
      ]
    }
  ],
  "author": "J.R José Rodrigues",
  "observation": "Pode ser que tenha outro co-autor"
}
