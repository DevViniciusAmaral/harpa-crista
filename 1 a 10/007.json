{
  "number": "7",
  "title": "Cristo Cura Sim!",
  "data": [
    {
      "type": "verse",
      "lines": [
        "Contra os males deste mundo,",
        "Deus nos vale só;",
        "Não há mal que Deus não cure,",
        "Pois de nós tem dó."
      ]
    },
    {
      "type": "chorus",
      "lines": [
        "Cristo cura, sim,",
        "Cristo cura, sim.",
        "Seu amor por nós é Imenso;",
        "Ele cura, sim!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Derramou Seu sangue puro",
        "P'ra remir a mim;",
        "Quando ungido sou de azeite,",
        "Sou curado, enfim!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Só noss’alma é bem segura,",
        "Oculta em Jesus;",
        "Ele o bálsamo da vida,",
        "Derramou na cruz."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Glória a Deus! Eterna glória,",
        "Demos-Lhe louvor;",
        "Glória, cânticos e hosanas",
        "Dai ao Redentor!"
      ]
    }
  ],
  "author": "Desconhecido",
  "observation": "Pode ser que tenha outro co-autor"
}
