{
  "number": 69,
  "title": "Jesus Quebrou os Meus Grilhões",
  "data": [
    {
      "type": "verse",
      "lines": [
        "Jesus quebrou os meus grilhões,",
        "Com sangue me comprou;",
        "A minha alma, de prisões,",
        "Eterna resgatou."
      ]
    },
    {
      "type": "chorus",
      "lines": [
        "Aleluia! Cristo salva o perdido pecador!",
        "Aleluia! Cristo salva o perdido com amor!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "É minha paz, e proteção,",
        "Meu bom Consolador;",
        "Auxílio certo na aflição,",
        "Remédio para a dor,"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Nenhum perigo temerei,",
        "Estando em Jesus;",
        "As dores não mais sofrerei;",
        "Jesus é minha luz."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Jesus, Teu nome louvarei,",
        "Com todo o coração;",
        "Alegre ao mundo anunciarei",
        "Tão grande salvação."
      ]
    }
  ],
  "author": "P.L.M Paulo Leivas Macalão",
  "observation": "Pode ser que tenha outro co-autor"
}
