{
  "number": 29,
  "title": "O Precioso Sangue",
  "data": [
    {
      "type": "verse",
      "lines": [
        "Oh! que precioso sangue,",
        "Meu Senhor verteu,",
        "Quando, para resgatar-nos,",
        "Padeceu!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Oh! que precioso sangue,",
        "Sangue de Jesus,",
        "Que por nós, foi derramado",
        "Sobre a cruz!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Oh! que precioso sangue,",
        "Sangue remidor!",
        "Sim, com este nos remiste,",
        "Redentor!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Oh! que precioso sangue,",
        "Sangue expiador!",
        "Eis o que da pena !ivra",
        "O malfeitor!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Oh! que precioso sangue,",
        "Purificador!",
        "Que de toda a mancha lava",
        "O pecador!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Oh! que precioso sangue,",
        "Fala-nos de paz;",
        "Tudo quanto a lei exige,",
        "Satisfaz!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Oh! que precioso sangue.",
        "Por Ele entrarei",
        "Sem receio, na presença",
        "Do meu Rei!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Oh! que precioso sangue,",
        "Do bom Salvador!",
        "Hoje, a todos manifesta",
        "Seu amor!"
      ]
    }
  ],
  "author": "H.M.W H. Maxwell Wrigth",
  "observation": "Pode ser que tenha outro co-autor"
}
